# 12 MHz clock
set_io clk        35

# RS232
set_io ser_rx      6    # 13B
set_io ser_tx      9    # 16A

# SPI Flash
set_io flash_clk  15
set_io flash_csb  16
set_io flash_io0  14
set_io flash_io1  17
set_io flash_io2  12
set_io flash_io3  13

# Onboard LEDs
set_io ledr_n     41
set_io ledg_n     40
set_io ledb_n     39

# external push button 
set_io -pullup yes BTN1  23   # 37A
